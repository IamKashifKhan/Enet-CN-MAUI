             x:Class="EnetCNMAUI.Views.OffersPage"
             xmlns:local="clr-namespace:EnetCNMAUI.RazorViews"
             Shell.NavBarIsVisible="False"
                xmlns:viewsBase="clr-namespace:EnetCNMAUI.Views.Base"
             Title="OffersPage">
    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type local:Offers}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>
</viewsBase:BasePage>
