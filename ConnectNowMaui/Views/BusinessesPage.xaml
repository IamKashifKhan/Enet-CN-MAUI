<?xml version="1.0" encoding="utf-8" ?>
<viewsBase:BasePage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ConnectNow.Views.BusinessesPage"
             xmlns:local="clr-namespace:ConnectNow.RazorViews"
             Shell.FlyoutBehavior="Disabled"
             xmlns:viewsBase="clr-namespace:ConnectNow.Views.Base"
            Shell.NavBarIsVisible="False">
    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent x:Name="root" Selector="#app" ComponentType="{x:Type local:Businesses}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>
</viewsBase:BasePage>